{
  "components": {
    "AudioPlayer": {
      "path": "./src/components/AudioPlayer",
      "props": {
        "src": "string",
        "controls": "boolean",
        "autoplay": "boolean",
        "loop": "boolean"
      },
      "description": "A reusable audio player component for playing audio files."
    },
    "FileUploader": {
      "path": "./src/components/FileUploader",
      "props": {
        "accept": "string",
        "multiple": "boolean",
        "onUpload": "function"
      },
      "description": "A file uploader component for uploading audio files."
    },
    "ProgressBar": {
      "path": "./src/components/ProgressBar",
      "props": {
        "progress": "number",
        "color": "string",
        "height": "number"
      },
      "description": "A progress bar component to show the progress of audio processing."
    },
    "Button": {
      "path": "./src/components/Button",
      "props": {
        "label": "string",
        "onClick": "function",
        "disabled": "boolean",
        "variant": "string"
      },
      "description": "A reusable button component with customizable styles."
    },
    "Modal": {
      "path": "./src/components/Modal",
      "props": {
        "isOpen": "boolean",
        "onClose": "function",
        "title": "string",
        "children": "ReactNode"
      },
      "description": "A modal component for displaying dialogs or messages."
    },
    "Header": {
      "path": "./src/components/Header",
      "props": {
        "title": "string",
        "logo": "string",
        "onMenuClick": "function"
      },
      "description": "A header component for the application with a logo and title."
    },
    "Footer": {
      "path": "./src/components/Footer",
      "props": {
        "text": "string",
        "links": "array"
      },
      "description": "A footer component with copyright text and links."
    }
  },
  "themes": {
    "light": {
      "primaryColor": "#3498db",
      "secondaryColor": "#2ecc71",
      "backgroundColor": "#ffffff",
      "textColor": "#2c3e50"
    },
    "dark": {
      "primaryColor": "#9b59b6",
      "secondaryColor": "#1abc9c",
      "backgroundColor": "#34495e",
      "textColor": "#ecf0f1"
    }
  },
  "settings": {
    "defaultTheme": "light",
    "componentPrefix": "AI"
  }
}
